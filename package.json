{
  "name": "check-package-dependency-changes",
  "version": "1.0.2",
  "description": "GitHub Action to detect dependency changes to determine whether to run tasks like security dependency scanners",
  "main": "dist/index.js",
  "scripts": {
    "test": "tsc -p tsconfig.test.json && jest index.test.js",
    "start": "tsc -p tsconfig.build.json && node ./lib/index.js",
    "build": "tsc -p tsconfig.build.json && ncc build lib/index.js -o dist --source-map --license licenses.txt",
    "package": "npm run build"
  },
  "keywords": [
    "github-action",
    "dependencies",
    "security",
    "package",
    "dependency-checker"
  ],
  "author": "ProductTech",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/check-package-dependency-changes"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@vercel/ncc": "^0.38.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@actions/artifact": "^2.1.0",
    "@actions/core": "^1.10.1"
  }
}
